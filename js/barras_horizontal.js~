/*
    Este script representa un gr√°fico de barras horizontal.
*/
(function($){
    $.barra_horizonal = function(){
        var object = this;
		var ops = object.options;
		object.div = object.element;

		object.canvas = $(object.div).find("canvas")[0];
		object.contexto = object.canvas.getContext("2d");
    }
}(jQuery));
